            <div class="fl_left">
                <ul>
                    <li><i class="fa fa-phone"></i> +56 (9) 12345678</li>
                    <li><i class="fa fa-envelope-o"></i> ServiExpres@servexp.com</li>
                </ul>
            </div>

            <div class="fl_right">
                <ul>
                    <li><a href="../HOME"><i class="fa fa-lg fa-home"></i></a></li>
                    <li><a href="../Test/Login">iniciar</a></li>
                    <li><a href="../Cliente/Create">Registrase</a></li>

            <div class="navbar-collapse collapse">
                <ul class="nav navbar-nav">
                    <li>@Html.ActionLink("Inicio", "Index", "Home")</li>
                    <li>@Html.ActionLink("Acerca de", "About", "Home")</li>
                    <li>@Html.ActionLink("Contacto", "Contact", "Home")</li>
                    <li>@Html.ActionLink("Reservar", "../Reserva")</li>
